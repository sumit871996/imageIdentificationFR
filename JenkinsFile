pipeline 
{
     agent {
     label 'imageidentififr'
     }
     stages{
         stage('Checkout') {
          steps{
               git 'https://github.com/sumit871996/imageIdentificationFR.git'
               }
          }
          
          stage('Docker build') {
          
               sh 'docker image build -f imageauthentifr/Dockerfile -t 'sumithpe/imageidentifront' imageauthentifr/'
               
          }
          
          
          
     }
    
   
}
