pipeline 
{
     agent {
     label 'imageidentififr'
     }
     stages{
         stage('Checkout') {
          steps{
               git 'https://github.com/sumit871996/imageIdentificationFR.git'
               }
          }
          
          stage('Docker build') {
          steps{
               sh 'docker image build -t sumithpe/imageidentifront .'
               }
               
          }
          
          
          
     }
    
   
}
